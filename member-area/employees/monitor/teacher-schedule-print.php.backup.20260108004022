<td bgcolor="<?php echo color('2','26'); ?>" class="auto-style2"><?php echo abc('2','26'); ?></td>
<td bgcolor="<?php echo color('3','26'); ?>" class="auto-style2"><?php echo abc('3','26'); ?></td>
