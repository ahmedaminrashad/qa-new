					$bgcolor ='#CDE7FF';
				}		
					$st1 = MYSQL_RESULT($result,$i,"stime_s_id");
			 if ($st1 == $tr){	
					$c = '#81F781';
						}
			else {	
					$c = '#ffffff';
						}
